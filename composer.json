{
  "name": "typo3-canto/canto-fal",
  "type": "typo3-cms-extension",
  "description": "Adds Canto FAL driver.",
  "homepage": "https://github.com/TYPO3-Canto/canto-fal",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "TYPO3 CMS",
    "Canto",
    "FAL"
  ],
  "authors": [
    {
      "name": "Nicole Hummel",
      "email": "nicole-typo3@nimut.dev",
      "role": "Developer"
    },
    {
      "name": "Tim Schreiner",
      "role": "Developer",
      "email": "tim.schreiner@km2.de"
    },
    {
      "name": "Denis Doerner",
      "role": "Developer",
      "email": "d.doerner@ecentral.de"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.9.2",
    "typo3/cms-core": "^13.4",
    "typo3/cms-filelist": "^13.4",
    "typo3/cms-filemetadata": "^13.4",
    "psr/log": "^3.0.1",
    "typo3-canto/canto-api": "^1.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "php-parallel-lint/php-parallel-lint": "^1.4",
    "phpstan/phpstan": "^1.2",
    "typo3/testing-framework": "^8.2.3"
  },
  "autoload": {
    "psr-4": {
      "TYPO3Canto\\CantoFal\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TYPO3Canto\\CantoFal\\Tests\\": "Tests/"
    }
  },
  "replace": {
    "typo3-ter/canto-fal": "self.version"
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    },
    "bin-dir": ".Build/bin",
    "vendor-dir": ".Build/vendor",
    "sort-packages": true
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "canto_fal",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "app-dir": ".Build",
      "web-dir": ".Build/public"
    }
  }
}
